<table id="profile_index" class="section_table">
  <thead>
    <tr>
      <th></th>
      <th>Name</th>
      <th>Type</th>
      <th>Email</th>
      <th>Contact Number</th>
      <th>Address</th>
      <% if can? :destroy, Profile %>
      <th></th>
      <% end %>
    </tr>
  </thead>

  <tbody>

    <% @profiles.each do |profile| %>
      <%=render profile%>
    <% end if @profiles%>
  </tbody>
</table>