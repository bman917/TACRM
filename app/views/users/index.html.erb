<h1>Users</h1>

<table class="mini">
<tr>
	<th>Role</th>
	<th>User name</th>
	<th>Email </th>
	<th></th>
</tr>

<% @users.each do | user | %>
<tr>
	<td> <%=user.role%></td>
	<td> <%=user.username%></td>
	<td> <%=user.email %></td>
	 <td><%= link_to 'Edit', edit_user_path(user) %></td>
</tr>
<% end %>
</table>

<%=link_to 'Add User', new_user_path %>