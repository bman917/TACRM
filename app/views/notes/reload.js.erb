modal_form_close();
<% if params[:source] == 'show' %>
  $('#notes_list').remove();
  $('#note_details').append('<%=j render(partial: 'notes/table', 
    locals: {notes: @profile.notes, remote: true})%>');
  $('#note_<%=@note.id%>').addClass('new');
  adjustHeight();

  $('#notes_list .add_overlay').on('click', function() {
    overlay();
  });

<% end %>